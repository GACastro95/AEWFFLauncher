<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>AEWFF_Launcher</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>aew.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Button_Template.png" />
    <None Remove="T_TitleRosterBG.png" />
    <None Remove="T_title_screen_AEWFF_Logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="aew.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Button_Template.png" />
    <Resource Include="T_TitleRosterBG.png" />
    <Resource Include="T_title_screen_AEWFF_Logo.png" />
  </ItemGroup>

</Project>
